<![CDATA[
left join
  odf_multi_valued_lookups multv
on
  multv.pk_id              = ${1:tab.col_id_da_instancia}
and upper(multv.object)    = upper('${2:code_do_objeto}')
and upper(multv.attribute) = upper('${3:code_do_atributo}')
